{
  "name": "svelte-typescript",
  "version": "1.0.0",
  "dependencies": {
    "core-js": "^3.0.0",
    "ejs": "^2.6.1",
    "esm": "^3.2.22",
    "express": "^4.16.4",
    "rimraf": "^2.6.3",
    "svelte": "^2.16.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@storybook/addon-actions": "^5.0.5",
    "@storybook/addon-knobs": "^5.0.5",
    "@storybook/addon-notes": "^5.0.5",
    "@storybook/addons": "^5.0.5",
    "@storybook/svelte": "^5.0.5",
    "@types/node": "^11.12.2",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.1.5",
    "glob": "^7.1.3",
    "nodemon": "^1.18.10",
    "rollup": "^1.7.4",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript": "^1.0.1",
    "svelte-loader": "^2.13.3",
    "typescript": "^3.4.1"
  },
  "scripts": {
    "clean": "rimraf build && rimraf public",
    "build:server": "cross-env NODE_ENV=production tsc && node svelte-build.js",
    "build:client": "rollup -c rollup.config.js",
    "build": "npm run clean && npm run build:server && npm run build:client",
    "start": "npm run build && node -r esm build/server.js",
    "dev": "nodemon --watch src --ext ts 'npm run start'",
    "storybook": "start-storybook --ci"
  }
}
